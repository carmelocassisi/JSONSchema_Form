{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "/schemas/geographical-location.schema.json",
	"title": "Longitude, Latitude and Quote",
	"description": "A geographical coordinate on a planet (most commonly Earth).",
	"required": ["latitude", "longitude"],
	"type": "object",
	"properties": {
		"latitude": {
			"$id": "#/properties/latitude",
			"type": "number",
			"title": "Latitude",
			"minimum": -90,
			"maximum": 90
		},
		"longitude": {
			"$id": "#/properties/longitude",
			"type": "number",
			"title": "Longitude",
			"minimum": -180,
			"maximum": 180
		},
		"quote": {
			"$id": "#/properties/quote",
			"type": "number",
			"title": "Quote"
		}
	}
}
